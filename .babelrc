{
  "ignore": [
    "/\/core-js/",
    "/@babel\b/"
  ],
  "presets": [
    [
      "@babel/preset-env",
      {
        "useBuiltIns": "entry",
        "targets": {
          "browsers": "> 1%, IE 11, not dead"
        }
      }
    ]
  ],
  "overrides": [
    {
      "test": "./node_modules",
      "sourceType": "unambiguous"
    }
  ],
  "plugins": [
    "@babel/plugin-transform-arrow-functions",
    "@babel/plugin-transform-runtime"
  ]
}